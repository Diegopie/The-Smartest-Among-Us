<!-- Header -->
<header>
  <section class="container cont">
    <article class="row">
      <h2 class="col-12" id="user">{{ username }}<span id="rubbish">'s Quizzes</span></h2>
      <h3 id="title"></h3>
    </article>
  </section>
</header>
<!-- Page Content -->
<main id="main"class="container">
  <!-- Display User Quizzes -->
  <section class="row" id="btns">
    {{#each quizzes}}
    {{> quizName}}
    {{/each}}
  </section>
  <section id="play-cont" class="row hide">
    <button  id="play" class="button">Play</button>
  </section>
  <section id="quiz" class="row hide">
    <article id="score-cont" class="cont col-12">
      <h3>Score: <span id="score">0</span></h3>
    </article>
  </section>

