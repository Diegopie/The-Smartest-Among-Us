<!-- Header -->
<header>
  <section class="container cont">
    <article class="row">
      <h2 class="">Account</h2>
    </article>
    <article class="row">
      <h3 class="col-12" id="user">{{ username }}</h3>
      <button class="button" id="Logout">Logout</button>
    </article>
  </section>
</header>
<!-- Page Content -->
<main class="container">
  <!-- Display User Quizzes -->
  <section class="row">
    {{#each quizzes}}
      {{> quizName}}
    {{/each}}
  </section>
  <!-- Display Create Account If Not Logged In -->
  <section class="row">
    <button class="col-6 button"><a class="text" href="./cre-account.html">Create Account</a></button>
  </section>
</main>